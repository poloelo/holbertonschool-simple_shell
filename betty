#ifndef SHELL_H
#define SHELL_H

#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <unistd.h>
#include <sys/types.h>
#include <sys/wait.h>

/* Function prototypes */
int _strlen(char *s);
int _strcmp(char *s1, char *s2);
char *_strdup(char *str);
void execute_command(char *line, char **env);
char **parse_line(char *line);
void free_array(char **array);

#endif /* SHELL_H */
